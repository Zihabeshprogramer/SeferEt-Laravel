# Amadeus API Configuration
# Add these to your .env file

# Amadeus API Credentials
# Get your credentials from https://developers.amadeus.com/
# IMPORTANT: Replace with your actual credentials - DO NOT commit real values!
AMADEUS_API_KEY=your_amadeus_api_key_here
AMADEUS_API_SECRET=your_amadeus_api_secret_here

# Environment: test or production
AMADEUS_ENV=test

# Optional: Custom log channel for Amadeus API calls
AMADEUS_LOG_CHANNEL=daily
